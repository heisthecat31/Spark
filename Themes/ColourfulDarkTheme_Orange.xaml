<ResourceDictionary x:Class="TheRThemes.ColourfulDarkTheme_Orange"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- Colourful theme  Colours -->

    <SolidColorBrush x:Key="ControlPrimaryDarkerBackground" Color="#FFc32b61" />
    <SolidColorBrush x:Key="ControlPrimaryDarkerBorderBrush" Color="#FFc32b61" />
    <SolidColorBrush x:Key="ControlPrimaryDefaultBackground" Color="#FFea6192" />
    <SolidColorBrush x:Key="ControlPrimaryDefaultBorderBrush" Color="#FFea6192" />
    <SolidColorBrush x:Key="ControlPrimaryBrightDefaultBackground" Color="#FFffaac9" />
    <SolidColorBrush x:Key="ControlPrimaryBrightDefaultBorderBrush" Color="#FFffaac9" />
    <SolidColorBrush x:Key="ControlPrimaryDisabledBackground" Color="#FFb85a7d" />
    <SolidColorBrush x:Key="ControlPrimaryDisabledBorderBrush" Color="#FFb85a7d" />
    <SolidColorBrush x:Key="ControlPrimaryMouseOverBackground" Color="#FFffaac9" />
    <SolidColorBrush x:Key="ControlPrimaryMouseOverBorderBrush" Color="#FFea6192" />
    <SolidColorBrush x:Key="ControlPrimarySelectedBackground" Color="#FFc32b61" />
    <SolidColorBrush x:Key="ControlPrimarySelectedBorderBrush" Color="#FFc32b61" />
    <SolidColorBrush x:Key="ControlPrimarySelectedMouseOverBackground" Color="#FFea6192" />
    <SolidColorBrush x:Key="ControlPrimarySelectedMouseOverBorderBrush" Color="#FFffaac9" />
    <SolidColorBrush x:Key="ControlPrimaryCaretSelectionBackground" Color="#FFea6192" />
    <SolidColorBrush x:Key="ControlPrimaryCaretBackground" Color="#FFffaac9" />
    <SolidColorBrush x:Key="ControlPrimaryGlythColour" Color="#FFffaac9" />
    <SolidColorBrush x:Key="ControlPrimaryMouseOverGlythColour" Color="#FFea6192" />
    <SolidColorBrush x:Key="ControlPrimarySelectedGlythColour" Color="#FFffaac9" />
    <SolidColorBrush x:Key="ControlPrimarySelectedMouseOverGlythColour" Color="#FFffaac9" />
    <SolidColorBrush x:Key="ControlPrimaryDisabledGlythColour" Color="#FFb85a7d" />

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="ColourfulDarkTheme_base.xaml"></ResourceDictionary>
    </ResourceDictionary.MergedDictionaries>

</ResourceDictionary>